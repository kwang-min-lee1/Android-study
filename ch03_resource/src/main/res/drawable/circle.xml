<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    >
<!-- 타원 또는 원형 만들기
    solid: 도형 색상 지정
    stroke: 도형 윤곽선 지정
-->
    <size
        android:width="100dp"
        android:height="100dp"
        />

    <solid
        android:color="#FF0"
        />

    <stroke
        android:color="@color/black"
        android:width="3dp"
        />

</shape>